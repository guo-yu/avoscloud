{
  "name": "avoscloud",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "serve": "node_modules/.bin/serve .",    
    "watch": "node_modules/.bin/rewatch src/*.js -c 'npm run build'",
    "build": "node_modules/.bin/uglifyjs src/avoscloud.js --mangle --compress -o dist/avoscloud.min.js --source-map dist/avoscloud.min.js.map --source-map-url avoscloud.min.js.map --source-map-root ../src/avoscloud.js",
    "dev": "npm run serve & npm run watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/turingou/avoscloud.git"
  },
  "keywords": [
    "avos",
    "avoscloud"
  ],
  "author": "turing <o.u.turing@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/turingou/avoscloud/issues"
  },
  "homepage": "https://github.com/turingou/avoscloud",
  "devDependencies": {
    "serve": "~1.4.0",
    "uglify-js": "^2.4.15",
    "rewatch": "~0.2.2"
  }
}
